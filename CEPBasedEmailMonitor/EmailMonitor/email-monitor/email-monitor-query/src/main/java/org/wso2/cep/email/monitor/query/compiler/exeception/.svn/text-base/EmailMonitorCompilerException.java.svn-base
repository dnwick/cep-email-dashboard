package org.wso2.cep.email.monitor.query.compiler.exeception;


public class EmailMonitorCompilerException extends Exception {

    public EmailMonitorCompilerException() {
    }

    public EmailMonitorCompilerException(String message) {
        super(message);
    }

    public EmailMonitorCompilerException(String message, Throwable e) {
        super(message, e);
    }

    public EmailMonitorCompilerException(Throwable e) {
        super(e);
    }




}
